  "scripts": {
    "dev": "nodemon src/server.js",
    "debug": "nodemon src/server.js",
    "queue": "nodemon src/queue.js --inspect",
    "build": "sucrase ./src -d ./dist --transforms imports",
    "prestart": "sequelize db:migrate",
    "start": "NODE_ENV=production node dist/server.js",
    "pretest": "NODE_ENV=test sequelize db:migrate",
    "test": "NODE_ENV=test jest  --coverage --coverageReporters=text-lcov --detectOpenHandles --forceExit",
    "posttest": "NODE_ENV=test sequelize db:migrate:undo:all",
    "precoveralls": "NODE_ENV=test sequelize db:migrate",
    "coveralls": "NODE_ENV=test jest  --coverage --coverageReporters=text-lcov --detectOpenHandles --forceExit  | coveralls",
    "postcoveralls": "NODE_ENV=test sequelize db:migrate:undo:all",
    "pretest:watch": "NODE_ENV=test sequelize db:migrate",
    "test:watch": "NODE_ENV=test jest --watch --detectOpenHandles",
    "posttest:watch": "NODE_ENV=test sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bee-queue": "^1.2.2",
